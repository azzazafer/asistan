{
    "version": 2,
    "framework": "nextjs",
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "crons": [
        {
            "path": "/api/cron/closer",
            "schedule": "0 9 * * *"
        },
        {
            "path": "/api/cron/process-delayed",
            "schedule": "* * * * *"
        }
    ]
}